#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/wuzhenhao/Desktop/JS/react/nextjs-tailwind-contentlayer-blog-starter/node_modules/.pnpm/contentlayer@0.3.4_esbuild@0.19.2/node_modules/contentlayer/bin/node_modules:/Users/wuzhenhao/Desktop/JS/react/nextjs-tailwind-contentlayer-blog-starter/node_modules/.pnpm/contentlayer@0.3.4_esbuild@0.19.2/node_modules/contentlayer/node_modules:/Users/wuzhenhao/Desktop/JS/react/nextjs-tailwind-contentlayer-blog-starter/node_modules/.pnpm/contentlayer@0.3.4_esbuild@0.19.2/node_modules:/Users/wuzhenhao/Desktop/JS/react/nextjs-tailwind-contentlayer-blog-starter/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/wuzhenhao/Desktop/JS/react/nextjs-tailwind-contentlayer-blog-starter/node_modules/.pnpm/contentlayer@0.3.4_esbuild@0.19.2/node_modules/contentlayer/bin/node_modules:/Users/wuzhenhao/Desktop/JS/react/nextjs-tailwind-contentlayer-blog-starter/node_modules/.pnpm/contentlayer@0.3.4_esbuild@0.19.2/node_modules/contentlayer/node_modules:/Users/wuzhenhao/Desktop/JS/react/nextjs-tailwind-contentlayer-blog-starter/node_modules/.pnpm/contentlayer@0.3.4_esbuild@0.19.2/node_modules:/Users/wuzhenhao/Desktop/JS/react/nextjs-tailwind-contentlayer-blog-starter/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/cli.cjs" "$@"
else
  exec node  "$basedir/../../bin/cli.cjs" "$@"
fi
